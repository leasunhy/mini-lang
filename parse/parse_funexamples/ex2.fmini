int main () {
  test (even(36)) ;
  test (odd(36)) ;
  test (even(37)) ;
  test (odd(37)) ;
}

bool even (int x) {
  if (x == 0) return true ; else return (odd(x-1)) ;
}

bool odd (int x) {
  if (x == 0) return false ; else return (even(x-1)) ;
}

void test (bool cond) {
  if (cond) print (1) ; else print (0) ;
}

